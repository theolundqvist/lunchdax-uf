    "firebase-admin": "^6.1.0",
    "firebase-functions": "^2.1.0",
    "firebaseui": "^3.4.1",
    "firestore": "^1.1.6",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "jquery-cropper": "^1.0.0",
    "pica": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-promise": "^3.6.0"
